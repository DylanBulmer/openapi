{
  "name": "@dylanbulmer/openapi",
  "version": "1.0.7",
  "main": "index.js",
  "author": "Dylan Bulmer <dylan@bulmersolutions.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest --config jest.config.json --passWithNoTests --coverage",
    "build": "yarn clean && swc src -d lib && tsc && cp package.json lib/",
    "clean": "rm -rf ./lib",
    "format": "prettier --write \"src/**/*.(ts|js)\"",
    "lint": "eslint -c .eslintrc.json --ignore-path .eslintignore --ext .ts src",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.224",
    "@swc/jest": "^0.2.22",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.5.3",
    "jest": "^28.1.2",
    "openapi-types": "^11.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4"
  }
}
